{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Resource</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ entity.subject }}</td>
            </tr>
            <tr>
                <th>Sports</th>
                <td>{{ entity.sports }}</td>
            </tr>
            <tr>
                <th>Companytype</th>
                <td>{{ entity.companyType }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ entity.country }}</td>
            </tr>
            <tr>
                <th>Province</th>
                <td>{{ entity.province }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ entity.city }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Updatetime</th>
                <td>{{ entity.updateTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Reporttime</th>
                <td>{{ entity.reportTime|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Department</th>
                <td>{{ entity.department }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('resource') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('resource_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
